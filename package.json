		"@types/d3-array": "^3.0.5",
		"@types/d3-scale": "^4.0.3",
		"@types/file-saver": "^2.0.3",
		"@types/jest": "^29.5.3",
		"@types/node": "^20.4.4",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@types/uuid": "^9.0.2",
		"@typescript-eslint/eslint-plugin": "^6.1.0",
		"@typescript-eslint/parser": "^6.1.0",
		"autoprefixer": "^10.4.14",
		"babel-jest": "^29.6.1",
		"concurrently": "^8.2.0",
