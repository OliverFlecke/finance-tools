		"@visx/legend": "^3.3.0",
		"@visx/mock-data": "^3.3.0",
		"@visx/responsive": "^3.3.0",
		"@visx/scale": "^3.3.0",
		"@visx/shape": "^3.3.0",
		"@visx/xychart": "^3.3.0",
		"d3-array": "^3.2.4",
		"dotenv": "^16.3.1",
		"file-saver": "^2.0.5",
		"immutability-helper": "^3.1.1",
		"react": "^18.1.0",
		"react-async-hook": "^4.0.0",
		"react-dnd": "^16.0.1",
		"react-dnd-html5-backend": "^16.0.1",
		"react-dom": "^18.1.0",
		"react-hook-form": "^7.45.2",
		"react-icons": "^4.10.1",
		"react-number-format": "^5.2.2",
		"react-spring": "^9.7.2",
		"uuid": "^9.0.0",
		"web-vitals": "^3.4.0"
	},
	"devDependencies": {
		"@babel/core": "^7.22.9",
		"@babel/preset-env": "^7.22.9",
