		"postcss-cli": "^10.1.0",
		"postcss-import": "^15.1.0",
		"postcss-nested": "^6.0.1",
		"prettier": "^3.0.0",
		"prettier-plugin-tailwindcss": "^0.4.1",
		"start-server-and-test": "^2.0.0",
		"tailwindcss": "^3.3.3",
		"ts-node": "^10.2.1",
		"typescript": "^5.1.6"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
